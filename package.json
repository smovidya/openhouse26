{
  "name": "openhouse25",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "wrangler types && astro dev",
    "start": "wrangler types && astro dev",
    "build": "wrangler types && astro check && astro build",
    "preview": "wrangler types && astro preview",
    "astro": "astro",
    "cf-types": "wrangler types",
    "db:generate": "bun drizzle-kit generate",
    "d1:migrate": "bun wrangler d1 migrations apply openhouse26_db --local",
    "d1:migrate:remote": "bun wrangler d1 migrations apply openhouse26_db --remote",
    "auth:generate": "bun x @better-auth/cli@latest generate --output ./src/db/schema/auth.schema.ts --config ./src/auth/index.ts -y"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.6.9",
    "@astrojs/svelte": "^7.2.0",
    "astro": "^5.14.1",
    "better-auth": "^1.3.24",
    "better-auth-cloudflare": "^0.2.6",
    "drizzle-orm": "^0.44.5",
    "svelte": "^5.39.7",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.24",
    "drizzle-kit": "^0.31.5"
  },
  "patchedDependencies": {
    "better-auth-cloudflare@0.2.5": "patches/better-auth-cloudflare@0.2.5.patch"
  }
}