{
  "name": "openhouse25",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "wrangler types && astro dev",
    "start": "wrangler types && astro dev",
    "build": "wrangler types && astro check && astro build",
    "preview": "wrangler types && astro preview",
    "deploy:staging": "bunx wrangler deploy --env staging",
    "deploy:production": "bunx wrangler deploy --env production",
    "astro": "astro",
    "cf-types": "wrangler types",
    "db:generate": "bun drizzle-kit generate",
    "db:studio": "bun drizzle-kit studio",
    "d1:migrate": "bun wrangler d1 migrations apply openhouse26_db --local",
    "d1:migrate:remote": "bun wrangler d1 migrations apply openhouse26_db --remote",
    "auth:generate": "bun x @better-auth/cli@latest generate --output ./src/db/schema/auth.schema.ts --config ./src/auth/index.ts -y"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.6.9",
    "@astrojs/svelte": "^7.2.0",
    "@bwip-js/browser": "^4.7.0",
    "@bwip-js/generic": "^4.7.0",
    "@libsql/client": "^0.15.15",
    "@paralleldrive/cuid2": "^2.2.2",
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/svelte-form": "^1.23.5",
    "@types/bwip-js": "^3.2.3",
    "arktype": "^2.1.22",
    "astro": "^5.14.1",
    "better-auth": "^1.3.26",
    "better-auth-cloudflare": "^0.2.6",
    "bwip-js": "^4.7.0",
    "carbon-icons-svelte": "^13.5.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.6",
    "formsnap": "^2.0.1",
    "svelte": "^5.39.8",
    "sveltekit-superforms": "^2.27.2",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.26",
    "@tailwindcss/typography": "^0.5.19",
    "daisyui": "^5.1.27",
    "drizzle-kit": "^0.31.5"
  },
  "patchedDependencies": {
    "better-auth-cloudflare@0.2.5": "patches/better-auth-cloudflare@0.2.5.patch"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide"
  ]
}